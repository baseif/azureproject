{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
  
     
    "tenantPassword": {
      "type": "string"
    },
  
  
  

  
  
  
  
  
  },
  "variables": {},
  "resources": [
  
  
  
    
    
    
    
    {
      "type": "Microsoft.Compute/virtualMachines/extensions",

      "name": "[Nova-Demo-vm-3/TrendMicroDSALinux]",
      "apiVersion": "2015-06-15",
      "location": "[resourceGroup().location]",
      "dependsOn": [
        "['Microsoft.Compute/virtualMachines/Nova-Demo-vm-3']"
      ],
      "properties": {

        "publisher": "TrendMicro.DeepSecurity",
        "type": "TrendMicroDSALinux",
        "typeHandlerVersion": "9.6",
        "autoUpgradeMinorVersion": true,
        "settings": {
          "DSMname": "[parameters('DSMname')]",
          "DSMport": "[parameters('DSMport')]",
          "policyNameorID": "[parameters('policyNameorID')]"
        },
        "protectedSettings": {
          "tenantID": "[parameters('tenantID')]",
          "tenantPassword": "[parameters('tenantPassword')]"

        }
      }
  
    }
    
    
  
  
  ],
  "outputs": {
    "result": {
        "value": "Hello World",
        "type" : "string"
    }
  }
}




